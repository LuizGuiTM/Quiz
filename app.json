{
  "expo": {
    "name": "Quiz App",
    "slug": "QuizApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/quiz-32.png",
    "splash": {
      "image": "./assets/quiz-32.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/quiz-32.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "QuizApp.apk"
    },
    "web": {
      "favicon": "./assets/quiz-32.png"
    }
  }
}
